{
  "expo": {
    "name": "ginfinity",
    "description": "Garten Infinity Cross platform application",
    "slug": "ginfinity",
    "owner": "garten",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#6C24AA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.garten.ginfinity"
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "galio-framework"
          ]
        }
      }
    },
    "githubUrl": "https://github.com/ohmygreen/ginfinity",
    "android": {
      "package": "com.garten.ginfinity"
    }
  }
}
